<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="353.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetwork.controller.ReceivedRequestController">
    <children>
        <TableView fx:id="tableView" prefHeight="300.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="usernameColumn" prefWidth="150.0" text="Username" />
                <TableColumn fx:id="firstNameColumn" prefWidth="150.0" text="First Name" />
                <TableColumn fx:id="lastNameColumn" prefWidth="150.0" text="Last Name" />
            <TableColumn fx:id="dateTimeColumn" prefWidth="150.0" text="Date Time" />
            </columns>
        </TableView>
        <Button layoutX="164.0" layoutY="308.0" mnemonicParsing="false" onAction="#handleAccept" prefHeight="31.0" prefWidth="95.0" text="Accept">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font></Button>
      <Button layoutX="349.0" layoutY="308.0" mnemonicParsing="false" onAction="#handleReject" prefHeight="31.0" prefWidth="95.0" text="Reject">
         <font>
            <Font name="Times New Roman Bold" size="14.0" />
         </font></Button>
    </children>
</AnchorPane>
